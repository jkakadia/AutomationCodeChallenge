{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": false,
    "outDir": "tmp",
    "strict": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "removeComments": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "asyncAwait",
    "plugins.ts"
  ]
}